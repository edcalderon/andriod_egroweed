<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andriod.egroweed">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight">

        <activity android:name="com.andriod.egroweed.view.StudentMenu"></activity>
        <activity android:name="com.andriod.egroweed.view.MainActivityLogin"></activity>
        <activity android:name="com.andriod.egroweed.view.Profile"></activity>
        <activity android:name="com.andriod.egroweed.view.EgrowerDashboard"></activity>
        <activity android:name="com.andriod.egroweed.view.EgrowerMasterDashboard"></activity>
        <activity android:name="com.andriod.egroweed.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>